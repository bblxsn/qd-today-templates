[
    {
        "comment": "登录",
        "request": {
            "method": "POST",
            "url": "https://www.kejiwanjia.net/wp-json/jwt-auth/v1/token",
            "headers": [
                {
                    "name": "Connection",
                    "value": "keep-alive"
                },
                {
                    "name": "Pragma",
                    "value": "no-cache"
                },
                {
                    "name": "Cache-Control",
                    "value": "no-cache"
                },
                {
                    "name": "Accept",
                    "value": "application/json, text/plain, */*"
                },
                {
                    "name": "Content-Type",
                    "value": "application/x-www-form-urlencoded"
                },
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/95.0.4636.4 Safari/537.36"
                },
                {
                    "name": "Origin",
                    "value": "https://www.kejiwanjia.net"
                },
                {
                    "name": "Referer",
                    "value": "https://www.kejiwanjia.net/"
                },
                {
                    "name": "Accept-Encoding",
                    "value": "gzip, deflate, br"
                },
                {
                    "name": "Accept-Language",
                    "value": "zh-CN,zh;q=0.9,en;q=0.8"
                }
            ],
            "cookies": [],
            "data": "nickname=&username={{username|urlencode}}&password={{password|urlencode}}&code=&img_code=&invitation_code=&token=&smsToken=&luoToken=&confirmPassword=&loginType=",
            "mimeType": "application/x-www-form-urlencoded"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [
                {
                    "re": "\"code\"",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "token",
                    "re": "\"token\":\"(.+?)\"",
                    "from": "content"
                },
                {
                    "name": "userKey",
                    "re": "\"link\":\"https:\\\\/\\\\/www.kejiwanjia.net\\\\/u\\\\/(.+?)\"",
                    "from": "content"
                }
            ]
        }
    },
