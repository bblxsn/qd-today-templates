            "headers": [],
            "cookies": [],
            "data": ""
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "jf",
                    "re": "积分</em>(.+?)<",
