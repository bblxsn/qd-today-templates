[
    {
        "request": {
            "method": "POST",
            "url": "https://www.vrmoo.net/wp-json/jwt-auth/v1/token",
            "headers": [
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; rv:122.0) Gecko/20100101 Firefox/122.0"
                }
            ],
            "cookies": [],
            "data": "username={{Email}}&password={{password}}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "user_display_name",
                    "from": "content"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "token",
                    "re": "token\":\"(.+?)\"",
                    "from": "content"
                },
                {
                    "name": "user",
                    "re": "user_display_name\":\"(.+?)\"",
                    "from": "content"
                },
                {
                    "name": "uid",
                    "re": "id\":\"(\\d+)\"",
                    "from": "content"
                }
            ]
        }
    },
    {
        "request": {
            "method": "POST",
            "url": "https://www.vrmoo.net/wp-json/b2/v1/userMission",
            "headers": [
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; rv:122.0) Gecko/20100101 Firefox/122.0"
                },
                {
                    "name": "Authorization",
                    "value": "Bearer {{token}}"
                }
            ],
            "cookies": [],
            "data": ""
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [],
            "extract_variables": []
        }
    },
    {
        "request": {
            "method": "POST",
            "url": "https://www.vrmoo.net/wp-json/b2/v1/getUserMission",
            "headers": [
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; rv:122.0) Gecko/20100101 Firefox/122.0"
                },
                {
                    "name": "Authorization",
                    "value": "Bearer {{token}}"
                }
            ],
            "cookies": [],
            "data": ""
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "sj",
                    "re": "date\":\"(.+?)\"",
                    "from": "content"
                },
                {
                    "name": "jf",
                    "re": "credit\":\"(\\d+)\"",
                    "from": "content"
                },
                {
                    "name": "zjf",
                    "re": "my_credit\":\"(\\d+)\"",
                    "from": "content"
                }
            ]
        }
    },
    {
        "comment": "Unicode转换",
        "request": {
            "method": "POST",
            "url": "api://util/unicode",
            "headers": [],
            "cookies": [],
            "data": "html_unescape=false&content=用户名：{{user}} 用户ID：{{uid}} 签到时间：{{sj}} 当日签到获得积分：{{jf}} 总积分：{{zjf}}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "__log__",
                    "re": "\"转换后\": \"(.*)\"",
                    "from": "content"
                }
            ]
        }
    }
]